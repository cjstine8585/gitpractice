#include <stdio.h>

int main()
{

  /* body */
  double number;

  printf("Please enter a number: ");
  scanf("%lf", &number);

  int i = 2;
  while(i<number){
    if(i==(number-1)
       printf("1");
    if(number%i==0)
      printf("0");

  }

  return (0);
}
